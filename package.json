  "license": "MIT",
  "type": "module",
  "bin": "bin/cli.js",
  "exports": "./index.js",
  "dependencies": {
    "@apollo/federation": "^0.25.1",
    "@graphql-tools/schema": "^7.1.5",
    "@graphql-tools/utils": "^7.10.0",
    "@grpc/grpc-js": "^1.3.2",
    "@grpc/proto-loader": "^0.6.2",
    "apollo-server": "^2.25.1",
    "chokidar": "^3.5.1",
